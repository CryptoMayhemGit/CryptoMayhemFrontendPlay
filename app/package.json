    "stylelint-order": "^5.0.0",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.1",
    "typescript": "~5.4.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,html,css}": "eslint --fix"
  }
}
