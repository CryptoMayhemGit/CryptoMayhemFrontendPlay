{
  "name": "crypto-mayhem-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "nx",
    "start": "npm run fix:styles && npx nx serve",
    "test": "nx run-many --target=test --all --colors=true",
    "lint": "ng lint && npm run lint:styles",
    "lint:styles": "npx stylelint **/*.scss",
    "fix:code": "prettier-eslint --write 'src/**/*.{js,jsx}' ",
    "fix:styles": "npx stylelint **/*.scss --fix",
    "e2e": "npx nx e2e crypto-mayhem-frontend-e2e",
    "format:fix": "pretty-quick --staged",
    "precommit": "run-s format:fix lint",
    "build:production": "npx nx build --configuration=production",
    "build:development": "npx nx build --configuration=development",
    "build:stage": "npx nx build --configuration stage",
    "watch": "npx nx build --watch --configuration development",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.6",
    "@angular/cdk": "^17.3.6",
    "@angular/common": "^17.3.6",
    "@angular/compiler": "^17.3.6",
    "@angular/core": "^17.3.6",
    "@angular/forms": "^17.3.6",
    "@angular/material": "^17.3.6",
    "@angular/platform-browser": "^17.3.6",
    "@angular/platform-browser-dynamic": "^17.3.6",
    "@angular/router": "^17.3.6",
    "@cookie3/analytics": "^0.6.0",
    "@fortawesome/angular-fontawesome": "^0.10.2",
    "@fortawesome/fontawesome-svg-core": "^6.1.0",
    "@fortawesome/free-solid-svg-icons": "^6.1.0",
    "@haechi-labs/face-sdk": "^1.10.0",
    "@haechi-labs/face-types": "^1.10.0",
    "@metamask/detect-provider": "^1.2.0",
    "@ngneat/transloco": "^4.1.0",
    "@ngneat/transloco-messageformat": "^4.1.0",
    "@ngrx/effects": "^17.2.0",
    "@ngrx/store": "^17.2.0",
    "@ngrx/store-devtools": "^17.2.0",
    "@nrwl/angular": "18.3.3",
    "@walletconnect/ethereum-provider": "^2.11.2",
    "@walletconnect/modal": "^2.6.2",
    "@walletconnect/universal-provider": "^2.11.2",
    "@walletconnect/web3wallet": "^1.10.2",
    "assert": "2.0.0",
    "buffer": "^6.0.3",
    "cipher-base": "^1.0.4",
    "ethers": "^5.6.8",
    "graphql": "^16.6.0",
    "https-browserify": "^1.0.0",
    "ngx-infinite-scroll": "^17.0.0",
    "os-browserify": "^0.3.0",
    "rxjs": "7.8.1",
    "stream-browserify": "3.0.0",
    "stream-http": "3.2.0",
    "tslib": "^2.4.0",
    "url": "0.11.0",
    "util": "0.12.4",
    "web3": "^1.7.4",
    "web3connect": "^1.0.0-beta.33",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.6",
    "@angular-devkit/core": "^17.3.6",
    "@angular-devkit/schematics": "^17.3.6",
    "@angular-eslint/eslint-plugin": "~13.2.1",
    "@angular-eslint/eslint-plugin-template": "~13.2.1",
    "@angular-eslint/template-parser": "~13.2.1",
    "@angular/cli": "^17.3.6",
    "@angular/compiler-cli": "^17.3.6",
    "@angular/language-service": "~14.0.0",
    "@nrwl/cypress": "18.3.3",
    "@nrwl/eslint-plugin-nx": "18.3.3",
    "@nrwl/jest": "18.3.3",
    "@nrwl/linter": "18.3.3",
    "@nrwl/workspace": "18.3.3",
    "@nx/angular": "18.3.4",
    "@nx/workspace": "18.3.4",
    "@schematics/angular": "^17.3.6",
    "@types/jest": "29.4.4",
    "@types/node": "18.16.9",
    "@typescript-eslint/eslint-plugin": "7.3.0",
    "@typescript-eslint/parser": "7.3.0",
    "cypress": "^9.1.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-cypress": "2.13.4",
    "husky": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "nx": "18.3.4",
    "prettier": "^2.6.2",
    "prettier-eslint": "^15.0.1",
    "prettier-eslint-cli": "^6.0.1",
    "pretty-quick": "^3.1.3",
