{
  "name": "crypto-mayhem-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "nx",
    "start": "npm run fix:styles && npx nx serve",
    "test": "npx nx test",
    "lint": "ng lint && npm run lint:styles",
    "lint:styles": "npx stylelint **/*.scss",
    "fix:code": "prettier-eslint --write 'src/**/*.{js,jsx}' ",
    "fix:styles": "npx stylelint **/*.scss --fix",
    "e2e": "npx nx e2e crypto-mayhem-frontend-e2e",
    "format:fix": "pretty-quick --staged",
    "precommit": "run-s format:fix lint",
    "build:production": "npx nx build",
    "build:stage": "npx nx build --configuration stage",
    "watch": "npx nx build --watch --configuration development",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.3.11",
    "@angular/common": "^13.3.11",
    "@angular/compiler": "^13.3.11",
    "@angular/core": "^13.3.11",
    "@angular/forms": "^13.3.11",
    "@angular/platform-browser": "^13.3.11",
    "@angular/platform-browser-dynamic": "^13.3.11",
    "@angular/router": "^13.3.11",
    "@ngneat/transloco": "^4.1.0",
    "@nrwl/angular": "^14.2.4",
    "ethers": "^5.6.8",
    "rxjs": "^7.5.5",
    "tslib": "^2.4.0",
    "zone.js": "^0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.3.6",
    "@angular-eslint/eslint-plugin": "~13.2.1",
    "@angular-eslint/eslint-plugin-template": "~13.2.1",
    "@angular-eslint/template-parser": "~13.2.1",
    "@angular/cli": "^13.3.7",
    "@angular/compiler-cli": "^13.3.11",
    "@angular/language-service": "~14.0.0",
    "@nrwl/cypress": "14.2.4",
    "@nrwl/eslint-plugin-nx": "14.2.4",
    "@nrwl/jest": "14.2.4",
    "@nrwl/linter": "14.2.4",
    "@nrwl/workspace": "14.2.4",
    "@types/jest": "27.4.1",
    "@types/node": "^12.20.55",
    "@typescript-eslint/eslint-plugin": "~5.24.0",
    "@typescript-eslint/parser": "~5.24.0",
    "cypress": "^9.1.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "jest": "27.5.1",
    "jest-preset-angular": "~11.1.2",
    "npm-run-all": "^4.1.5",
    "nx": "14.2.4",
    "prettier": "^2.6.2",
    "prettier-eslint": "^15.0.1",
    "prettier-eslint-cli": "^6.0.1",
    "pretty-quick": "^3.1.3",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-config-standard-scss": "^4.0.0",
    "stylelint-order": "^5.0.0",
    "ts-jest": "27.1.4",
    "ts-node": "~10.8.0",
    "typescript": "~4.5.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,html,css}": "eslint --fix"
  }
}
