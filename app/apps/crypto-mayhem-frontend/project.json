{
  "projectType": "application",
  "sourceRoot": "apps/crypto-mayhem-frontend/src",
  "prefix": "crypto-mayhem",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "options": {
        "outputPath": "dist/apps/crypto-mayhem-frontend",
        "index": "apps/crypto-mayhem-frontend/src/index.html",
        "main": "apps/crypto-mayhem-frontend/src/main.ts",
        "polyfills": "apps/crypto-mayhem-frontend/src/polyfills.ts",
        "tsConfig": "apps/crypto-mayhem-frontend/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          {
            "glob": "favicon.ico",
            "input": "libs/crypto-mayhem/assets/images",
            "output": "./"
          },
          {
            "glob": "staticwebapp.config.json",
            "input": "",
            "output": "./"
          },
          {
            "glob": "**/*",
            "input": "libs/crypto-mayhem/assets/",
            "output": "assets"
          }
        ],
        "styles": ["apps/crypto-mayhem-frontend/src/styles.scss"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "3mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "50kb",
              "maximumError": "100kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/crypto-mayhem-frontend/src/environments/environment.ts",
              "with": "apps/crypto-mayhem-frontend/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "2mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "50kb",
              "maximumError": "100kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/crypto-mayhem-frontend/src/environments/environment.ts",
              "with": "apps/crypto-mayhem-frontend/src/environments/environment.dev.ts"
            }
          ],
          "outputHashing": "all"
        },
        "stage": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "2mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "50kb",
              "maximumError": "100kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/crypto-mayhem-frontend/src/environments/environment.ts",
              "with": "apps/crypto-mayhem-frontend/src/environments/environment.stage.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "crypto-mayhem-frontend:build:production"
        },
        "develop": {
          "browserTarget": "crypto-mayhem-frontend:build:develop"
        },
        "stage": {
          "browserTarget": "crypto-mayhem-frontend:build:stage"
        },
        "development": {
          "browserTarget": "crypto-mayhem-frontend:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "crypto-mayhem-frontend:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/crypto-mayhem-frontend/src/**/*.ts",
          "apps/crypto-mayhem-frontend/src/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/crypto-mayhem-frontend"],
      "options": {
        "jestConfig": "apps/crypto-mayhem-frontend/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["type:app"]
}
